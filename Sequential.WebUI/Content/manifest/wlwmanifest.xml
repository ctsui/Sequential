<?xml version="1.0" encoding="utf-8" ?>
<manifest xmlns="http://schemas.microsoft.com/wlw/manifest/weblog">
   <options>
      <clientType>Metaweblog</clientType>
      <supportsPostAsDraft>Yes</supportsPostAsDraft>
      <supportsFileUpload>Yes</supportsFileUpload>
      <supportsCategories>Yes</supportsCategories>
      <supportsCustomDate>Yes</supportsCustomDate>
      <supportsCommentPolicy>Yes</supportsCommentPolicy>
      <supportsExtendedEntries>Yes</supportsExtendedEntries>
      <supportsExcerpt>Yes</supportsExcerpt>
      <supportsNewCategories>Yes</supportsNewCategories>
      <supportsNewCategoriesInline>Yes</supportsNewCategoriesInline>
      <supportsMultipleCategories>No</supportsMultipleCategories>
      <supportsHierarchicalCategories>No</supportsHierarchicalCategories>
      <supportsKeywords>Yes</supportsKeywords>
      <supportsPingPolicy>No</supportsPingPolicy>
      <supportsAuthor>No</supportsAuthor>
      <supportsSlug>No</supportsSlug>
      <supportsPassword>No</supportsPassword>
      <supportsTrackbacks>No</supportsTrackbacks>
      <supportsPages>No</supportsPages>
      <supportsPageParent>No</supportsPageParent>
      <supportsPageOrder>No</supportsPageOrder>
      <!--additional live writer options-->
      <supportsEmptyTitles>No</supportsEmptyTitles>
      <requiresHtmlTitles>No</requiresHtmlTitles>
      <requiresXHTML>No</requiresXHTML>
      <supportsScripts>Yes</supportsScripts>
      <supportsEmbeds>Yes</supportsEmbeds>
      <characterSet>UTF-8</characterSet>
      <maxCategoryNameLength>40</maxCategoryNameLength>
      <supportsAutoUpdate>Yes</supportsAutoUpdate>
      <supportsHttps>No</supportsHttps>
   </options>
   <weblog>
      <serviceName>Sequential MetaWeblog</serviceName>
      <homepageLinkText>Sequential 2011</homepageLinkText>
   </weblog>
   <views>
      <default>WebLayout</default>
      <view type="WebPreview" src="http://sputnikx.com/"/>
      <view type="WebLayout" src="WebLayout.html"/>
   </views>
</manifest>